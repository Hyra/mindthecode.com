.container {
  display: grid;
  max-width: 1100px;
  margin: 0 auto;
  grid-gap: 50px;
  grid-template-areas: "main aside";
  grid-template-columns: 800px 250px;
  padding: 0 20px; }

main {
  grid-area: main; }

aside {
  grid-area: aside;
  /* background: yellow; */
  margin-right: 0; }

.item {
  margin-bottom: 40px; }
  .item .category span {
    background-color: #fa7369;
    padding: 5px;
    color: white;
    font-family: DinProCond;
    font-size: 13px;
    text-transform: uppercase; }
  .item .post-date span {
